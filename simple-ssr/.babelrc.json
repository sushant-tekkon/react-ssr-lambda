{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": 12
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ],
    [
      "css-modules-transform",
      {
        "camelCase": true,
        "extensions": [".css", ".scss"]
      }
    ],
    "dynamic-import-node"
  ]
}
